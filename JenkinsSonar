pipeline {
    agent any

    tool name: 'maven3', type: 'maven'
    
    stages {
        stage('git checkout'){
            steps{
                git credentialsId: 'GitHub', url: 'https://github.com/mahi-442/Jenkins'
            }
        }
        stage('maven build') {
            steps{
                sh 'mvn clean package'
            }
        }
    }

}