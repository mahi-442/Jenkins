pipeline {
    agent any

    tools {
        maven 'maven3' 
    }

    stages {

        stage('parallel blocks') {
            parallel{
                stage(maven build) {
                    steps {
                        sh "mvn clean package"
                    }
                }
                stage(sonar7) {
                    withSonarQubeEnv('sonar7') {
                        sh 'mvn sonar:sonar'
                    }
                }
            }

        
        }
    }
}